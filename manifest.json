{
  "name": "forceeen",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Force Choose Language to Result of Google Search.",
  "action": {
    "default_icon": "icons/48.png",
    "default_popup": "option.html"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
   "permissions": [
    "tabs"
  ]
}
